# aliases.sh

# Directory navigation
alias ll='ls -alF'
alias la='ls -A'
alias ..='cd ..'
alias ...='cd ../..'

# Git shortcuts
alias gs='git status'
alias ga='git add .'
alias gc='git commit -m'
alias gp='git push'
alias gl='git log --oneline --graph --decorate --all'
alias gb='git branch'
alias gd='git diff'

# System utilities
alias cls='clear'
alias pyserve='python -m http.server 8080'
alias v='code .'
